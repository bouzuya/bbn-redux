{
  "name": "bbn-redux",
  "version": "0.1.0",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.0",
    "@types/react": "^16.14.4",
    "@types/react-dom": "^16.9.11",
    "@types/react-redux": "^7.1.16",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "chokidar-cli": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "homepage": "https://bouzuya.net/sandbox/bbn-redux/",
  "private": true,
  "scripts": {
    "build": "run-s 'build:format' 'build:react-scripts'",
    "build:format": "prettier --check '{src,test}/**/*.{css,ts,tsx}'",
    "build:react-scripts": "react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --write '{src,test}/**/*.{css,ts,tsx}'",
    "start": "run-p 'start:*'",
    "start:format": "chokidar '{src,test}/**/*.{ts,tsx}' --command 'npm run build:format' --silent",
    "start:react-scripts": "react-scripts start",
    "test": "react-scripts test"
  }
}
